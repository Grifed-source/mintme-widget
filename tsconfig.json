{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "ESNext",
	  "lib": ["DOM", "ESNext"],
	  "jsx": "react-jsx",
	  "moduleResolution": "bundler",
	  "allowSyntheticDefaultImports": true,
	  "esModuleInterop": true,
	  "declaration": true,
	  "declarationDir": "dist",
	  "outDir": "dist",
	  "strict": true,
	  "skipLibCheck": true
	},
	"include": ["src"]
  }
  